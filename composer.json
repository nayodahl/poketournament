{
    "name": "nayodahl/poketournament",
    "type": "project",
    "description": "A sandbox project to organize pokemon cards tournaments.",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/rate-limiter": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/stopwatch": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/ux-chartjs": "^1.2",
        "symfony/validator": "5.3.*",
        "symfony/web-profiler-bundle": "^5.2",
        "symfony/webpack-encore-bundle": "^1.9",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.5",
        "deployer/dist": "^6.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.81",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.21",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "5.3.*",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.23",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0.1"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
