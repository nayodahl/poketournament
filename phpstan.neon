parameters:
    level: max

    paths:
        - config
        - src
        - public
    
    symfony:
        container_xml_path: var/cache/dev/App_KernelDevDebugContainer.xml

    bootstrapFiles:
        - ./bin/.phpunit/phpunit/vendor/autoload.php

    doctrine:
        objectManagerLoader: config/phpstan/doctrine-orm-bootstrap.php